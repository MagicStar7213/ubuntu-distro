sed -i 's/ports.ubuntu/es.ports.ubuntu/g' /etc/apt/sources.list
sed -i 's/jammy/kinetic/g' /etc/apt/sources.list && \
cp /data/data/com.termux/files/home/ubuntu-distro/vncstop /usr/local/bin/ && mv /usr/local/bin/vncstop /usr/local/bin/vncserver-stop
cp /data/data/com.termux/files/home/ubuntu-distro/vncstart /usr/local/bin && mv /usr/local/bin/vncstart /usr/local/bin/vncserver-start
cp /data/data/com.termux/files/home/ubuntu-distro/vnc /usr/local/bin
exit
